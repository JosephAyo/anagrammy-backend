{
  "name": "connectme-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc -p tsconfig.prod.json",
    "build-dist": "./node_modules/.bin/ts-node ./pre-build/build.ts",
    "start": "node -r module-alias/register ./dist --node_env=production",
    "dev": "nodemon",
    "test": "yarn jest --clearCache && jest --verbose --runInBand"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@server": "dist/server",
    "@database": "dist/database",
    "@utils": "dist/utils",
    "@routes": "dist/routes",
    "@models": "dist/models",
    "@services": "dist/services",
    "@middlewares": "dist/middlewares",
    "@test-setup": "dist/test-setup"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.109.0",
    "@aws-sdk/lib-storage": "^3.141.0",
    "apple-auth": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "command-line-args": "^5.2.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "firebase-admin": "^11.0.1",
    "fs-extra": "^10.1.0",
    "google-auth-library": "^8.0.3",
    "helmet": "^5.0.2",
    "jet-logger": "^1.1.5",
    "jimp": "^0.16.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.7.5",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "socket.io": "^4.5.1",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.5.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^17.0.25",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@withshepherd/faker": "^5.5.5",
    "concurrently": "^7.1.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3"
  }
}
